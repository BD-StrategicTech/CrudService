    "type": "library",
    "license": "MIT",
    "description": "Eloquent model-independent CRUD Service",
    "keywords": ["Eloquent", "CRUD", "Laravel"],
    "homepage": "https://github.com/BD-StrategicTech/CrudService",
    "authors": [
        {
            "name": "Matt Frost",
            "email": "mattf@budgetdumpster.com",
            "homepage": "https://www.budgetdumpster.com",
            "role": "developer"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "BudgetDumpster\\": "src/"
        }
